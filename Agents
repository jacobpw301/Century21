				randomNumber = Math.floor(Math.random()*length1);
			if(randomNumber <= i){
				randomNumber = Math.floor(Math.random()*length1);
			}
			temp = agents[i];
			agents[i] = agents[randomNumber];
			agents[randomNumber] = temp;
		}
		var agentsList = document.createElement('ul');
		agentsList.classList.add("realtor-list", "small-block-grid-2", "medium-block-grid-3");
